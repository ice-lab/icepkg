{
  "name": "example-pkg-react-multi-components",
  "version": "0.0.0",
  "private": true,
  "files": [
    "esm",
    "cjs",
    "es2017",
    "dist",
    "build"
  ],
  "exports": {
    "./Button": {
      "es2017": {
        "types": "./es2017/Button/index.d.ts",
        "default": "./es2017/Button/index.js"
      },
      "import": {
        "types": "./esm/Button/index.d.ts",
        "default": "./esm/Button/index.js"
      }
    },
    "./Avatar": {
      "es2017": {
        "types": "./es2017/Avatar/index.d.ts",
        "default": "./es2017/Avatar/index.js"
      },
      "import": {
        "types": "./esm/Avatar/index.d.ts",
        "default": "./esm/Avatar/index.js"
      }
    }
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@ice/jsx-runtime": "^0.2.0",
    "@swc/helpers": "^0.4.14"
  },
  "devDependencies": {
    "@ice/pkg": "workspace:*",
    "@ice/pkg-plugin-docusaurus": "workspace:*",
    "example-pkg-plugin": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "style-unit": "^3.0.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
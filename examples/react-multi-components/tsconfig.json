  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react",
    "moduleResolution": "nodenext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "example-pkg-react-multi-components": ["./src"],
      "example-pkg-react-multi-components/*": ["./src/*"]

{
  "name": "@ice/pkg-plugin-docusaurus",
  "version": "1.4.2",
  "description": "Plugin for component previewing",
  "main": "es2017/index.mjs",
  "exports": {
    ".": {
      "type": "./es2017/index.d.mts",
      "import": "./es2017/index.mjs",
      "default": "./es2017/index.mjs"
    },
    "./remark/extractCode": {
      "type": "./es2017/extractCode.d.ts",
      "import": "./es2017/remark/extractCode.js",
      "default": "./es2017/remark/extractCode.js"
    },
    "./plugin.js": {
      "type": "./es2017/plugin.d.ts",
      "import": "./es2017/plugin.js",
      "default": "./es2017/plugin.js"
    },
    "./css/custom.css": "./es2017/css/custom.css"
  },
  "files": [
    "esm",
    "es2017",
    "!esm/**/*.map",
    "!es2017/**/*.map"
  ],
  "scripts": {
    "watch": "ice-pkg start",
    "build": "ice-pkg build",
    "lint": "pnpm eslint --ext .js,.jsx,.ts,.tsx,.mts,.mjs ./",
    "prepublishOnly": "pnpm run-s lint build"
  },
  "dependencies": {
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "@swc/helpers": "^0.4.3",
    "@ice/jsx-runtime": "^0.1.1-beta.1",
    "address": "^1.2.1",
    "consola": "^2.15.3",
    "copy-text-to-clipboard": "^3.0.1",
    "detect-port": "^1.3.0",
    "directory-tree": "^3.3.1",
    "docusaurus-plugin-less": "^2.0.2",
    "docusaurus-plugin-sass": "^0.2.2",
    "es-module-lexer": "^0.10.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "hast-util-find-and-replace": "3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "postcss-plugin-rpx2vw": "^0.0.3",
    "prism-react-renderer": "^1.3.1",
    "qrcode.react": "^3.1.0",
    "react-tooltip": "^4.2.21",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "sass": "^1.3.0",
    "sass-loader": "^12.6.0",
    "style-unit": "^3.0.4",
    "unified": "^10.1.2",
    "unist-util-visit": "2.0.3"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.9.1",
    "@types/react": "^17.0.0",
    "@ice/pkg": "^1.4.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

import AdaptorGenerate from '{{adaptorGenerate}}';

import Adaptor from '{{adaptor}}';

import '{{style}}';

if (Adaptor.name === '{{adaptorNamePkg}}') {
    AdaptorGenerate(Adaptor);
} else {
    const tip = `[\`${Adaptor.name}\` !== \`{{adaptorNamePkg}}\`]. The name in adaptor/index.js should be eaqual to componentConfig.name in package.json

    Please change the name and execute \`npm run start\``;
    document.getElementById('root').innerHTML = tip;
    console.error(tip);
}

{
  "name": "node",
  "version": "0.1.0",
  "description": "组件功能描述",
  "type": "module",
  "files": [
    "esm",
    "cjs",
    "es2017",
    "dist",
    "build"
  ],
  "module": "es2017/index.js",
  "exports": {
    ".": {
      "import": "./es2017/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "ice",
    "js",
    "library"
  ],
  "devDependencies": {
    "@ice/pkg": "^1.0.0",
    "@iceworks/spec": "^1.0.0",
    "eslint": "^7.0.0",
    "stylelint": "^13.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
